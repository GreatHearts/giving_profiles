<section class='feed col-md-6 col-md-offset-3'>
  <h1> Donations in the Yale Network</h1>


  <div class="fb-page" data-href="https://www.facebook.com/wwf" 
  data-tabs="timeline" 
  data-small-header="true" 
  data-adapt-container-width="true" 
  data-hide-cover="true" 
  data-show-facepile="false">
  <blockquote cite="https://www.facebook.com/wwf" class="fb-xfbml-parse-ignore"><a href="https://www.facebook.com/facebook">Facebook</a></blockquote>
  </div>

  <% my_network_donations.each do |don| %>
  <div class='card1' style='text-align: left'>
    <%#= don.to_json %>
    <div class='single_feed_item row'>
      <div class='col-md-3 ct '>
        <img src='/img/face.jpg' class='profile-pic-1'>
      </div>
      <div class='col-md-9' style='font-size:18px'>
        <div> <%=$users.random['name']%> donated <%=rand(1000)%>$ to The Red Cross. </div>
        <div class='fr' style='margin-top:8px'> October 12th, 15:12 (Yale)</div>
      </div>      
    </div>
  </div>
  <% end %>
</div>